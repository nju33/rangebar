{
  "name": "rangebar",
  "version": "0.0.2",
  "description": "ðŸ’Š Elements like input[type=range]",
  "main": "dist/rangebar.umd.js",
  "jsnext:main": "dist/rangebar.es.js",
  "scripts": {
    "start": "node dev-server.js",
    "prepublish": "yarn prod",
    "preprod": "mkdir -p dist; NODE_ENV='production' node prod.js",
    "prod": "babili dist/rangebar.js --no-comments --minified -o dist/rangebar.min.js",
    "_test": "xo && ava",
    "test": "xo"
  },
  "repository": "nju33/rangebar",
  "keywords": [
    "ui",
    "browser",
    "commonjs",
    "es",
    "range",
    "input"
  ],
  "author": "nju33",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nju33/rangebar/issues"
  },
  "homepage": "https://github.com/nju33/rangebar#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "ava": "^0.18.2",
    "babel-eslint": "7",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.2.2",
    "babili": "^0.0.12",
    "browser-sync": "^2.18.8",
    "casual": "^1.5.11",
    "cssnano": "^3.10.0",
    "gulp": "^3.9.1",
    "gulp-less": "^3.3.0",
    "gulp-postcss": "^6.4.0",
    "nightmare": "^2.10.0",
    "postcss-easings": "^0.3.0",
    "postcss-modules": "^0.6.4",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.0",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-string": "^2.0.2",
    "rollup-plugin-svelte": "^1.6.1",
    "xo": "^0.18.1"
  },
  "ava": {
    "files": [
      "test/specs/*.js"
    ]
  },
  "xo": {
    "env": [
      "node",
      "browser"
    ],
    "files": [
      "example/**/*.js",
      "src/**/*.js"
    ],
    "esnext": true,
    "space": 2,
    "ignores": [
      "es/**"
    ],
    "parser": "babel-eslint",
    "overrides": [
      {
        "files": "test/fixtures/*js",
        "esnext": false,
        "globals": [
          "rangebar"
        ]
      }
    ]
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "raf": "^3.3.0"
  }
}
